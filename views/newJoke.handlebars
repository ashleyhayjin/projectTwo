<div class="card">
  <div class="card-header">
    <h2>Welcome, {{username}}!</h2>
    <h3>Create a New Joke</h3>
  </div>
  <form class="card-body" id="new-joke-form">
    <label for="joke-content" class="form-label">New Joke:</label>
    <input class="form-input" type="text" id="joke-content" name="joke-content"/>
     
    <button type="submit" class="btn btn-primary">Create</button>
  </form>
</div>
<script src="/js/newJoke.js"></script>
