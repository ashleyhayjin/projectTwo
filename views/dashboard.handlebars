{{#if jokes.length}}
<section>
    <h2>Jokes:</h2>
     {{#each jokes as |joke|}}   
    <h4>{{joke.joke_text}}</h4>
        
{{/each}}
</section>
{{/if}}
{{#if logged_in}}
    <button type="submit" class="btn"><a class ="link" href="/jokes">New Joke</a></button>
    <button type="tweet" class="btn"><a class="link" href="/api/post">Tweet random Joke</a></button>
{{/if}}
<script src="/controllers/api/postto.js"></script>